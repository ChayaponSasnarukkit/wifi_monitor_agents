
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'soc/soc:pcie/pci0000:00/0000:00:01.0/0000:01:00.0'
	option channel 'auto'
	option band '5g'
	option htmode 'VHT80'
	option country 'CN'
	option cell_density '0'
	option txpower '20'
	option disabled '1'

config wifi-iface 'AP_radio0'
	option device 'radio0'
	option mode 'ap'
	option macaddr '14:91:82:2f:95:29'
	option network 'AP'
	option ssid 'OpenWrt_5G'
	option encryption 'psk2'
	option key 'xxreJMeExxzT8fI'
	option disabled '1'
	option ifname 'wlan0'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'soc/soc:pcie/pci0000:00/0000:00:02.0/0000:02:00.0'
	option band '2g'
	option htmode 'HT20'
	option country 'CN'
	option cell_density '0'
	option channel 'auto'
	option disabled '1'

config wifi-iface 'AP_radio1'
	option device 'radio1'
	option mode 'ap'
	option macaddr '14:91:82:2f:95:28'
	option ssid 'OpenWrt_2.4G'
	option network 'AP'
	option disabled '1'
	option encryption 'psk2'
	option key 'xxreJMeExxzT8fI'
	option ifname 'wlan1'

config wifi-iface 'client_radio1'
	option device 'radio1'
	option mode 'sta'
	option network 'wwan'
	option ssid 'OpenWrt_2.4G'
	option encryption 'psk2'
	option key 'xxreJMeExxzT8fI'
	option disabled '1'
	option ifname 'wlan1'

config wifi-iface 'client_radio0'
	option device 'radio0'
	option mode 'sta'
	option network 'wwan'
	option encryption 'psk2'
	option key 'xxreJMeExxzT8fI'
	option ssid 'OpenWrt_5G'
	option disabled '1'
	option ifname 'wlan0'

